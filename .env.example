# Database (SQLite - sin necesidad de PostgreSQL)
DATABASE_URL=sqlite:///./propchat.db

# Anthropic API (opcional, no en uso actualmente)
ANTHROPIC_API_KEY=your_anthropic_key_here

# App Config
APP_NAME=PropChat API
DEBUG=True
API_VERSION=v1

# CORS
FRONTEND_URL=http://localhost:3000

# Google Gemini API (REQUERIDO para el chat)
GEMINI_API_KEY=your_gemini_api_key_here

# Mapbox API (REQUERIDO para geocoding)
MAPBOX_TOKEN=your_mapbox_token_here
